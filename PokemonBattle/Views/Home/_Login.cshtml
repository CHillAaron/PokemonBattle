@model PokemonBattle.CORE.LoginUser

<form action="Login" method="post" class="col-5">
    <h2 class="text-center">Login</h2>
    <div class="form-group">
        <label asp-for="LogEmail"></label>
        <input asp-for="LogEmail" class="form-control" />
        <span class="text-danger" asp-validation-for="LogEmail"></span>
    </div>
    <div class="form-group">
        <label asp-for="LogPassword"></label>
        <input asp-for="LogPassword" class="form-control" />
        <span class="text-danger" asp-validation-for="LogPassword"></span>
    </div>
    <input type="submit" value="Log In" class="btn btn-success" />
</form>
